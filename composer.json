{
    "name": "decodelabs/genesis",
    "description": "Universal bootstrapping framework",
    "type": "library",
    "keywords": [ "bootstrap", "framework", "entry" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.0",

        "decodelabs/archetype": "^0.2.5",
        "decodelabs/atlas": "^0.10.5",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/fluidity": "^0.3.3",
        "decodelabs/glitch-support": "^0.4.1",
        "decodelabs/pandora": "^0.2.3",
        "decodelabs/terminus": "^0.9|^0.10",
        "decodelabs/veneer": "^0.10.10"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Genesis\\": "src/Genesis"
        },
        "files": [
            "src/global.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Genesis\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.7.x-dev"
        }
    }
}
