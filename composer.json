{
    "name": "decodelabs/genesis",
    "description": "Universal bootstrapping framework",
    "type": "library",
    "keywords": [ "bootstrap", "framework", "entry" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3",
        "decodelabs/atlas": "^0.12",
        "decodelabs/enumerable": "^0.2",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/fluidity": "^0.3.3",
        "decodelabs/glitch-support": "^0.5",
        "decodelabs/pandora": "^0.3",
        "decodelabs/terminus": "^0.11",
        "decodelabs/veneer": "^0.12.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Genesis\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Genesis\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.10.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
