{
    "name": "decodelabs/genesis",
    "description": "Universal bootstrapping framework",
    "type": "composer-plugin",
    "keywords": [ "bootstrap", "framework", "entry" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "repositories": {
    },
    "require": {
        "php": "^8.4",
        "composer-plugin-api": "^2.0",

        "decodelabs/archetype": "^0.4",
        "decodelabs/atlas": "^0.14",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/kingdom": "^0.2",
        "decodelabs/monarch": "^0.2",
        "decodelabs/slingshot": "^0.3",
        "decodelabs/terminus": "^0.14"
    },
    "require-dev": {
        "composer/composer": "^2.0",

        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/commandment": "^0.3.0",
        "decodelabs/systemic": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Genesis\\": "src/Genesis/"
        },
        "classmap": [
            "src/Genesis.php",
            "src/Commandment/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Genesis\\Tests\\": "tests/"
        }
    },
    "extra": {
        "class": "DecodeLabs\\Genesis\\Composer\\Plugin",
        "branch-alias": {
            "dev-develop": "0.14.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
